{
  "name": "RobotBet",
  "version": "1.0",
  "description": "",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/background.js"
  },
  "icons": {

  },
  "action": {
    "default_popup": "src/popup.html",
    "default_icon": "assets/img/icon.png"
  },
  "options_page": "src/options.html",
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "tabs",
    "notifications",
    "cookies"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*"
  ]
}
